// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model URLS {
  id      String @id @unique
  url     String
  user_id String
  visits  Int    @default(0)

  @@index([user_id])
}
